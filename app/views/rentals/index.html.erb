<h1>ブログ一覧</h1>

<table>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>住所</th>
    <th>築年数</th>
    <th>備考</th>
  </tr>

<% @rentals.each do |rental| %>
  <tr>
    <td><%= rental.property_name %></td>
    <td><%= rental.rent %></td>
    <td><%= rental.adress %></td>
    <td><%= rental.age %></td>
    <td><%= rental.note %></td>

    <td><%= link_to '詳細', rental_path(rental.id) %></td>
  </tr>
<% end %>
</table>
