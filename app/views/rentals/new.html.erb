<h1>物件登録</h1>

<%= form_for(@rental) do |form| %>
  <div class="rental_property">
     <p><%= form.label :物件名 %></p>
     <%= form.text_field :property_name %>
  </div>
  <div class="rental_property">
     <p><%= form.label :賃料 %></p>
     <%= form.text_field :rent %>
     <span> 円</span>
  </div>
  <div class="rental_property">
     <p><%= form.label :住所 %></p>
     <%= form.text_field :adress %>
  </div>
  <div class="rental_property">
     <p><%= form.label :築年数 %></p>
     <%= form.text_field :age %>
     <span> 年</span>
  </div>
  <div class="rental_property">
     <p><%= form.label :備考 %><p>
     <%= form.text_field :note %>
  </div>

  <%= form.submit %>
<% end %>
